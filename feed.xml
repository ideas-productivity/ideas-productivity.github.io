---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>{{ site.url | absolute_url | append: "/" }}</id>
	<title>{{ site.title | strip_html  }}{% if site.subtitle %}{{ " | " | append: site.subtitle | strip_html }}{% endif %}</title>
	{% if site.subtitle -%}
	<subtitle>{{- site.subtitle | strip_html -}}</subtitle>
	{%- endif %}
	<link href="{{ page.url | absolute_url }}" rel="self" type="application/atom+xml" />
	<link href="{{  ""  | absolute_url }}" rel="alternate" type="text/html" />
	<rights>(c) {{ site.time | date: '%Y' }} by the IDEAS Productivity family of projects, licensed CC-BY 4.0 International</rights>
  <icon>{{- "assets/images/favicon-32x32.png" | absolute_url -}}</icon>
  <logo>{{- "assets/images/android-chrome-192x192.png" | absolute_url -}}</logo>

{% comment %}}
  <channel>
      <title>{{ site.title | xml_escape }}</title>
      <link>{{ site.url }}</link>
      <atom:link href="{{ page.url | prepend: site.url }}" rel="self" type="application/rss+xml" xmlns:atom="https://www.w3.org/2005/Atom" />
      <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>

      {% for event in site.events %}
      <item>
        <title>{{ event.title | xml_escape }}</title>
        <link>{{ event.url | prepend: site.url | xml_escape }}</link>
        <startDate>{{ event.startdate | date: "%a, %b %d, %Y" }}</startDate>
        <endDate>{{ event.enddate | date: "%a, %b %d, %Y" }}</endDate>
        <description>{{ event.description | xml_escape }}</description>
        <registrationURL>{{ event.registration-url | xml_escape }}</registrationURL>
        <artifacts>
        {%- if include.format == "li" -%}
            {%- assign row=false -%}
        {%- else -%}
            {%- assign row = true -%}
        {%- endif -%}

        {%- for i in event.artifacts -%}
        {%- if i.status == "published" or i.status == nil -%}
          {%- unless row -%}<li><h4>{%- endunless -%}
            {%- if i.yt-video-id -%}<a href="{{ include.yt-url | default: site.youtube-watch-url }}{{ i.yt-video-id | xml_escape }}">
            {%- elsif i.yt-playlist-id -%}<a href="{{ include.yt-url | default: site.youtube-playlist-url }}{{ i.yt-playlist-id  | xml_escape }}">
            {%- elsif i.gh-repo -%}<a href="{{ include.gh-url | default: site.github-url | xml_escape }}/{{ i.gh-repo | xml_escape }}">
            {%- elsif i.doi -%}<a href="{{ include.doi-url | default: site.doi-resolver-url | xml_escape }}/{{ i.doi  | xml_escape }}">
            {%- elsif i.url -%}<a href="{{ i.url | xml_escape }}">{%- endif -%}
            {{ i.label | xml_escape }}
            {%- if i.yt-video-id or i.yt-playlist-id or i.gh-repo or i.doi or i.url -%}</a>{%- endif -%}
            {%- if i.note %} {{ i.note }}{%- endif -%}
            {%- if i.format %} ({{ i.format }}){%- endif -%}
            {% if row -%}
              {%- unless forloop.last %} | {% endunless -%}
            {%- else -%}
            </h4></li>
            {%- endif -%}
          {%- endif -%}
        {%- endfor -%}
        </artifacts>

      </item>
      {% endfor %}

  </channel>
{% endcomment %}
</feed>